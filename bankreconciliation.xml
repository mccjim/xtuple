 <querysource>
  <name>periodend</name>
  <sql>
select 'For GL Period Ending: ' || formatDate(period_end) AS rptperiod,
trialbal_ending * -1 AS glbalance
FROM bankrec
JOIN period ON period_id = getperiodid(bankrec_enddate)
JOIN bankaccnt ON bankrec_bankaccnt_id = bankaccnt_id
JOIN trialbal ON trialbal_accnt_id = bankaccnt_accnt_id AND period_id = trialbal_period_id

where  bankrec_id = &lt;? value("bankrec_id") ?></sql>
 </querysource>
 <querysource>
  <name>futureTrans</name>
  <sql>SELECT SUM(recitem) AS recitem FROM
(select  gltrans_amount * -1 AS recitem 
FROM bankrecitem
JOIN gltrans ON bankrecitem_source_id = gltrans_id AND bankrecitem_source = 'GL'
JOIN bankrec ON bankrec_id = bankrecitem_bankrec_id AND gltrans_date >
(SELECT  period_end FROM period WHERE period_id = getperiodid(bankrec_enddate)) 
AND bankrec_id =  &lt;? value("bankrec_id") ?>

UNION ALL

select  sltrans_amount * -1 AS recitem 
FROM bankrecitem
JOIN sltrans ON bankrecitem_source_id = sltrans_id AND bankrecitem_source = 'SL'
JOIN bankrec ON bankrec_id = bankrecitem_bankrec_id AND
(SELECT gltrans_date FROM gltrans WHERE gltrans_journalnumber = sltrans_gltrans_journalnumber LIMIT 1) >
(SELECT  period_end FROM period WHERE period_id = getperiodid(bankrec_enddate)) 
AND bankrec_id =  &lt;? value("bankrec_id") ?>) as balamt</sql>
 </querysource>
